{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "CatPy",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.catpy",
				"match": "\\b(i_també|també|com_a|anomenat|anomenat_com|anomenat_com_a|afirma|atura|atur-et|trenca|frena|classe|continua|funció|eliminar|destruir|del_contrari_si|en_cas_contrari_si|del_contrari|excepte|Fals|fals|finalment|per|per_a|des_de|global|si|en_cas_que|importa|dins|dins_de|és|lambda|Res|no_local|no_és|o_bé|o_per_el_contrari|ignora|pasa|eleva|puja|retorna|retornar|Veritable|veritable|Cert|cert|Certament|certament|prova|intenta|mentres|amb|rendiment|imprimir|imprimeix|fuet|lallargaria_de|allargaria|allargada|concatena|enganxa|afegeix|en)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.catpy",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.catpy",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "icrescenti"
}