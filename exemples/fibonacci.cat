funció fibonacci(n):
    en_cas_que n <= 0:
        retorna []
    en_cas_contrari_si n == 1:
        retorna [0]
    en_cas_contrari_si n == 2:
        retorna [0, 1]
    del_contrari:
        seqüència = [0, 1]
        mentres lallargaria_de(seqüència) < n:
            next_number = seqüència[-1] + seqüència[-2]
            seqüència.concatena(next_number)
        retorna seqüència

n = 10
seqüència = fibonacci(n)
imprimir(seqüència)
